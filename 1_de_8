#!/usr/bin/env python3
# -*- coding: utf-8 -*-
"""Compatibility wrapper for the renamed PENIN-Î© core module."""
from __future__ import annotations

import runpy
import warnings

try:
    from penin_omega_1_core import *  # type: ignore
except ImportError as exc:  # pragma: no cover - surfaced at import time
    raise ImportError("Failed to import penin_omega_1_core") from exc

try:
    from penin_omega_1_core import __all__ as __all__  # type: ignore
except ImportError:
    import penin_omega_1_core as _impl

    __all__ = tuple(name for name in vars(_impl) if not name.startswith("_"))
    del _impl

warnings.warn(
    "Module '1_de_8' has been renamed to 'penin_omega_1_core'. Please update your imports.",
    DeprecationWarning,
    stacklevel=2,
)

if __name__ == "__main__":
    runpy.run_module("penin_omega_1_core", run_name="__main__")
